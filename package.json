    "@types/node": "^20.8.6",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "astro": "^3.3.1",
    "changelogen": "^0.5.5",
    "clean-publish": "^4.2.0",
    "eslint": "^8.51.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-import": "^2.28.1",
