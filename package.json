    "figma-api": "^1.11.0",
    "lightningcss": "^1.22.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-gamut": "^1.3.3",
